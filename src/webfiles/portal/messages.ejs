<html>
<head>
    <link rel="stylesheet" type="text/css" href="<%= cdnURL %>/css/juxt.css">
    <script src="<%= cdnURL %>/js/pjax.min.js"></script>
    <script src="<%= cdnURL %>/js/juxt.js"></script>
    <!-- Google Analytics -->
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-195842548-1', 'auto');
        ga('send', 'pageview');
    </script>
    <!-- End Google Analytics -->

</head>
<body>
<%- include('nav_bar', { selection: 3 }); %>
<div id="main">
    <h1 id="title" class="page-header"><%= lang.global.messages %></h1>
    <div class="community-page-posts-wrapper">
        <p class="no-posts-text"><%= lang.messages.coming_soon %></p>

        <div class="col-lg-8">
            <style type="text/css">
                flipnote-player {
                    --flipnote-player-font-family: inherit;
                    --flipnote-player-button-background: #84db4d;
                    --flipnote-player-icon-color: #0f571c;
                    --flipnote-player-button-color: #84db4d;
                    --flipnote-player-slider-track: #84db4d;
                    --flipnote-player-slider-level: #0f571c;
                    --flipnote-player-slider-handle: #0f571c;
                }
            </style>
            <script src="https://cdn.jsdelivr.net/npm/flipnote.js@5/dist/flipnote.webcomponent.min.js" crossorigin="anonymous"></script>
            <flipnote-player id="flipnote" class="mb-4 shadow-sm rounded-lg w-100" src="https://kaeru.b-cdn.net/star/uploads/cc09018b53a441f09c91f50ed0decf4b/memo/cee0cqordtway3mngotmnwobaayc.kwz" width="640" controls="">
                <section class="mb-4 shadow-sm rounded-lg w-100">
                    <img width="640" src="https://kt-imogen.b-cdn.net/preview/cee0cqordtway3mngotmnwobaayc.gif" alt="sniper gaming" class="embed-responsive-item w-100">
                </section>
                <section class="alert-warning mb-4 p-4 rounded-lg shadow-sm" role="alert">
                    <h5 class="mt-0"><i aria-hidden="true" class="uil uil-exclamation-triangle"></i> Unsupported Browser</h5>
                    <p style="font-size: 15px;">Sound and playback control are unavailable because your browser doesn't support the modern web technologies needed to run our browser-based Flipnote player.</p>
                    <p class="mb-0" style="font-size: 15px;">For the full experience, please <a href="https://browser-update.org/update-browser.html">update your web browser</a>.</p>
                </section>
            </flipnote-player>

        </div>



    </div>
    <img src="" onerror="wiiuBrowser.showLoadingIcon(!1);window.scroll(0, 0);">
</div>
<body onload="stopLoading();wiiuBrowser.lockUserOperation(false);">
</body>
</html>